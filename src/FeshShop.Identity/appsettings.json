{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "App": {
    "Name": "Identity"
  },
  "MongoDbSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "Database": "feshShop-identityDb",
    "Seed": false
  },
  "SwaggerSettings": {
    "enabled": true,
    "name": "v1",
    "title": "Identity Layer",
    "version": "v1",
    "routePrefix": "docs"
  },
  "JwtSettings": {
    "secretKey": "MY_SECRET_KEY_MY_SECRET_KEY_MY_SECRET_KEY_MY_SECRET_KEY_MY_SECRET_KEY_MY_SECRET_KEY",
    "expiryMinutes": 30,
    "issuer": "feshShop-identity",
    "validateLifetime": true
  },
  "SerilogSettings": {
    "consoleEnabled": true,
    "level": "information"
  },
  "SeqSettings": {
    "Enabled": true,
    "Url": "http://localhost:5341",
    "Token": "secret"
  }
}
